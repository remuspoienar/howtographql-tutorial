{
    "extends": ["prettier"],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    },
    "plugins": ["prettier"],
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true
    }
}